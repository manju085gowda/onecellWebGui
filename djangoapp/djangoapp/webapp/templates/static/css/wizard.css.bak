

.wizard {
  visibility: hidden;
  width: 100%;
  display: block;
  padding: 0;
}
.wizard:before,
.wizard:after {
  content: " ";
  display: table;
}
.wizard:after {
  clear: both;
}
.wizard:before,
.wizard:after {
  content: " ";
  display: table;
}
.wizard:after {
  clear: both;
}
.wizard nav {
  position: relative;
  width: 100%;
  background-color: #f1f1f1;
}
.wizard-top-head{ font-size:17px; color:#595959; font-weight:700;}
.wizard-head{ font-size:17px; font-weight:400; color:#595959; width:100%; border-bottom:1px solid #eee; border-top:1px solid #eee; padding:15px 0px 15px 0px;}
p.step {
  background: #a3a3a3;
  border-radius: 0.8em;
  -moz-border-radius: 0.8em;
  -webkit-border-radius: 0.8em;
  color: #ffffff;
  display: inline-block;
  font-weight: normal; float:left;
  line-height: 1.6em;
  margin-right: 5px;
  text-align: center;
  width: 1.6em; font-size:12px;
}


.wizard-align {    
    position: relative;
    background: #FFFFFF;   
    border-radius: 4px;   
    margin: 60px auto 10px;
    overflow: hidden;}


.step-downarrow{ float:right}
.btn-toolbar {
    margin-left: -5px;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 1000;
    padding: 10px 20px;
    background: white;
    border-top: 1px solid #e7eaec;
}

.question {
border-bottom:1px solid #eee;
  padding: .7em 0em .7em 0em;
}
.question.error {
  background-color: #e74c3c;
}
.question.success {
  background-color: #2ecc71;
}


.question .question-headline {
  font-weight: 400;
  border-bottom: 0px solid #ddd;
  font-size: 14px;
  color: #000;
}
.question .question-answers {
  margin-top: .5em;
  color: #595959;
}
.question .question-answers .answer + .answer {
  margin-top: .2em;
}
.question .question-answers .answer input {
  display: inline-block;
  margin-right: .5em;
}
.question .question-answers .answer label {
  cursor: pointer;
}
label {
    display: inline-block;
    max-width: 100%;
    margin-bottom: 0px; 
    font-weight: 400; font-size:13px;
}
.result {
  margin-bottom: 2em;
  font-size: 1.2em;
  background-color: #3498db;
  border-radius: 10px;
  padding: 10px 20px;
  display: none;
}

.btn-check {
  padding: 10px 20px;
  background-color: #2ecc71;
  color: #fff;
  border-radius: 10px;
  border: 0;
  cursor: pointer;
  font-size: 1.5em;
}
.btn-check:hover {
  background-color: #1b7943;
}

.clear-config-btn{position:relative; left:265px;}













@media (min-width: 768px) {
  .wizard nav {
    float: left !important;
    width: 270px;
    height: 100%; position:fixed;

  }
}
